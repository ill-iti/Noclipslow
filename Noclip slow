--[[SLOW NOCLIP SYSTEM THIS NOCLIP REDUCES YOUR WALKSPEED TO AVOID TRIGGERING ANTI-CHEAT VELOCITY CHECKS SOME ANTI-CHEATS ONLY DETECT WHEN YOU MOVE TOO FAST THROUGH WALLS USE AT YOUR OWN RISK]]local Players=game:GetService("Players")local RunService=game:GetService("RunService")local TweenService=game:GetService("TweenService")local UserInputService=game:GetService("UserInputService")local player=Players.LocalPlayer;local character=player.Character or player.CharacterAdded:Wait()local humanoid=character:WaitForChild("Humanoid")local rootPart=character:WaitForChild("HumanoidRootPart")local noclipEnabled=false;local connection=nil;local NORMAL_SPEED=16;local NOCLIP_SPEED=8;local function enableSlowNoclip()if connection then return end;noclipEnabled=true;humanoid.WalkSpeed=NOCLIP_SPEED;connection=RunService.Stepped:Connect(function()pcall(function()for _,part in pairs(character:GetDescendants())do if part:IsA("BasePart")then part.CanCollide=false end end end)end)print("SLOW NOCLIP ENABLED WALKSPEED REDUCED TO "..NOCLIP_SPEED)end;local function disableSlowNoclip()noclipEnabled=false;if connection then connection:Disconnect()connection=nil end;humanoid.WalkSpeed=NORMAL_SPEED;pcall(function()for _,part in pairs(character:GetDescendants())do if part:IsA("BasePart")and part.Name~="HumanoidRootPart"then part.CanCollide=true end end;rootPart.CanCollide=false end)print("SLOW NOCLIP DISABLED WALKSPEED RESTORED")end;local screenGui=Instance.new("ScreenGui")screenGui.Name="SlowNoclipGui"screenGui.ResetOnSpawn=false;screenGui.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;screenGui.Parent=player:WaitForChild("PlayerGui")local mainFrame=Instance.new("Frame")mainFrame.Size=UDim2.new(0,240,0,90)mainFrame.Position=UDim2.new(0.5,-120,0.08,0)mainFrame.BackgroundColor3=Color3.fromRGB(18,18,18)mainFrame.BackgroundTransparency=0.1;mainFrame.BorderSizePixel=0;mainFrame.Active=true;mainFrame.Draggable=true;mainFrame.Parent=screenGui;local corner=Instance.new("UICorner")corner.CornerRadius=UDim.new(0,12)corner.Parent=mainFrame;local stroke=Instance.new("UIStroke")stroke.Color=Color3.fromRGB(255,200,100)stroke.Thickness=2;stroke.Transparency=0.7;stroke.Parent=mainFrame;local titleLabel=Instance.new("TextLabel")titleLabel.Size=UDim2.new(1,-20,0,22)titleLabel.Position=UDim2.new(0,10,0,5)titleLabel.BackgroundTransparency=1;titleLabel.Text="⚡ SLOW NOCLIP ⚡"titleLabel.TextColor3=Color3.fromRGB(255,200,100)titleLabel.TextSize=16;titleLabel.Font=Enum.Font.GothamBold;titleLabel.Parent=mainFrame;local warningLabel=Instance.new("TextLabel")warningLabel.Size=UDim2.new(1,-20,0,18)warningLabel.Position=UDim2.new(0,10,0,28)warningLabel.BackgroundTransparency=1;warningLabel.Text="Speed: 16→8 (ON) | 8→16 (OFF)"warningLabel.TextColor3=Color3.fromRGB(200,200,200)warningLabel.TextSize=11;warningLabel.Font=Enum.Font.Gotham;warningLabel.TextXAlignment=Enum.TextXAlignment.Left;warningLabel.Parent=mainFrame;local statusLabel=Instance.new("TextLabel")statusLabel.Size=UDim2.new(0,140,0,25)statusLabel.Position=UDim2.new(0,10,0,52)statusLabel.BackgroundTransparency=1;statusLabel.Text="Status: DISABLED"statusLabel.TextColor3=Color3.fromRGB(255,100,100)statusLabel.TextSize=13;statusLabel.Font=Enum.Font.GothamBold;statusLabel.TextXAlignment=Enum.TextXAlignment.Left;statusLabel.Parent=mainFrame;local toggleButton=Instance.new("TextButton")toggleButton.Size=UDim2.new(0,70,0,28)toggleButton.Position=UDim2.new(1,-80,0,51)toggleButton.BackgroundColor3=Color3.fromRGB(60,60,60)toggleButton.Text="ENABLE"toggleButton.TextColor3=Color3.fromRGB(255,255,255)toggleButton.TextSize=12;toggleButton.Font=Enum.Font.GothamBold;toggleButton.Parent=mainFrame;local buttonCorner=Instance.new("UICorner")buttonCorner.CornerRadius=UDim.new(0,8)buttonCorner.Parent=toggleButton;local function updateUI(enabled)local tweenInfo=TweenInfo.new(0.25,Enum.EasingStyle.Quad,Enum.EasingDirection.Out)if enabled then statusLabel.Text="Status: ACTIVE"statusLabel.TextColor3=Color3.fromRGB(100,255,150)toggleButton.Text="DISABLE"TweenService:Create(toggleButton,tweenInfo,{BackgroundColor3=Color3.fromRGB(255,100,100)}):Play()TweenService:Create(stroke,tweenInfo,{Color=Color3.fromRGB(100,255,150)}):Play()else statusLabel.Text="Status: DISABLED"statusLabel.TextColor3=Color3.fromRGB(255,100,100)toggleButton.Text="ENABLE"TweenService:Create(toggleButton,tweenInfo,{BackgroundColor3=Color3.fromRGB(60,60,60)}):Play()TweenService:Create(stroke,tweenInfo,{Color=Color3.fromRGB(255,200,100)}):Play()end end;toggleButton.MouseButton1Click:Connect(function()if noclipEnabled then disableSlowNoclip()updateUI(false)else enableSlowNoclip()updateUI(true)end end)UserInputService.InputBegan:Connect(function(input,processed)if processed then return end;if input.KeyCode==Enum.KeyCode.N then if noclipEnabled then disableSlowNoclip()updateUI(false)else enableSlowNoclip()updateUI(true)end end end)player.CharacterAdded:Connect(function(newChar)character=newChar;humanoid=character:WaitForChild("Humanoid")rootPart=character:WaitForChild("HumanoidRootPart")if noclipEnabled then disableSlowNoclip()updateUI(false)end end)print("SLOW NOCLIP SYSTEM LOADED SUCCESSFULLY PRESS N KEY OR CLICK BUTTON TO TOGGLE WARNING MOVE SLOWLY TO REDUCE DETECTION RISK")
